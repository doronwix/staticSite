{
    "ecmaFeatures": {
        "globalReturn": false
    },
    "parser": "espree",
    "plugins": [
        "noko"
    ],
    "env": {
        "browser": true,
        "amd": true,
        "jquery": true,
        "qunit": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "local",
                "varsIgnorePattern": "ko|self|myself|data|inheritedInstance|args",
                "args": "after-used",
                "argsIgnorePattern": "require|ko|params|data|result|^caller|args|response|e|event|ui|element|valueAccessor|allBindingsAccessor|viewModel|bindingContext",
                "caughtErrors": "none"
            }
        ],
        "no-eq-null": 2,
        "no-console": 2,
        "no-alert": 2,
        "no-debugger": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-redeclare": 2,
        "no-invalid-this": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-return-assign": 2,
        "no-ex-assign": 2,
        "no-class-assign": 2,
        "no-func-assign": 2,
        "no-native-reassign": 2,
        "no-extend-native": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-new-require": 2,
        "no-unreachable": 2,
        "no-unexpected-multiline": 2,
        "new-parens": 2,
        "valid-typeof": 2,
        "use-isnan": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-dupe-class-members": 2,
        "no-delete-var": 2,
        "no-eval": 2,
        "no-implied-eval": 2,
        "no-proto": 2,
        "no-iterator": 2,
        "no-caller": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-obj-calls": 2,
        "no-throw-literal": 2,
        "no-void": 2,
        "no-with": 2,
        "no-array-constructor": 2,
        "no-constant-condition": 2,
        "no-extra-boolean-cast": 2,
        "no-implicit-coercion": [
            2,
            { "boolean": false }
        ],
        "no-labels": 2,
        "no-sparse-arrays": 2,
        "no-empty-pattern": 2,
        "no-floating-decimal": 2,
        "no-empty": [
            2,
            { "allowEmptyCatch": true }
        ],
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "guard-for-in": 2,
        "no-unused-expressions": [
            2,
            { "allowTernary": true }
        ],
        "no-multi-str": 2,
        "no-duplicate-case": 2,
        "no-case-declarations": 2,
        "no-fallthrough": 2,
        "no-empty-character-class": 2,
        "no-regex-spaces": 2,
        "no-invalid-regexp": 2,
        "no-control-regex": 2,
        "no-extra-semi": 2,
        "no-inner-declarations": 2,
        "no-negated-in-lhs": 2,
        "no-useless-call": 2,
        "no-extra-bind": 2,
        "consistent-return": 2,
        "no-else-return": 1,
        "no-param-reassign": 1,
        "no-lonely-if": 1,
        "no-self-assign": 1,
        "no-useless-escape": 1,
        "no-useless-concat": 1,
        "no-useless-computed-key": 1,
        "no-unmodified-loop-condition": 1,
        "no-unsafe-finally": 1,
        "no-ternary": 0,
        "no-nested-ternary": 2,
        "no-unneeded-ternary": 2,
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1,
                "VariableDeclarator": {
                    "var": 1,
                    "const": 1,
                    "let": 1
                }
            }
        ],
        "eqeqeq": 1,
        "no-multiple-empty-lines": [
            2,
            {
                "max": 2
            }
        ],
        "no-use-before-define": 1,
        "no-mixed-spaces-and-tabs": 1,
        "comma-dangle": 1,
        "comma-spacing": 1,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "no-multi-spaces": 1,
        "space-infix-ops": 1,
        "vars-on-top": 1,
        "no-magic-numbers": 1,
        "semi": 1,
        "quotes": 1,
        "strict": 0,
        "no-catch-shadow": 0,
        "linebreak-style": 0,
        "no-trailing-spaces": 0,
        "no-irregular-whitespace": 0
    },
